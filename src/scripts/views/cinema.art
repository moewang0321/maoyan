<div class="body-wrap">
    <!-- 城市切换||搜索 -->
    <div class="topbar">
        <div class="gray-bg topbar-bg ">
            <div class="city-entry">
                <span class="city-name">{{city}}</span>
                <i class="city-entry-arrow"></i>
            </div>
            <div class="search-entry search-input" data-type="cinema">
                <img
                    src="data:image/png;base64,">
                <span>搜影院</span>
            </div>
        </div>

    </div>
    <!-- 筛选 -->
    <div class="nav-wrap filter-nav-wrap">
        <div class="tab mb-line-b">
            <div class="item" data-tab=".region">全城<span class="drop"></span></div>
            <div class="item" data-tab=".brand">品牌<span class="drop"></span></div>
            <div class="item" data-tab=".special">特色<span class="drop"></span></div>
        </div>

    </div>

    <!-- 影院列表 -->
    <div class="cinema-list">
        <div class="list-wrap">
            
        </div>

    </div>
    <div class="locatebanner">
        <img class="locatebanner-arrow" src="data:image/png;base64," alt="">
        <span class="locatebanner-content">定位失败，请稍后重试</span>
        <div class="locatebanner-refresh-wrap">
            <img class="locatebanner-refresh" src="data:image/png;base64,">
        </div>
    </div>
</div>